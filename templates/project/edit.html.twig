{% extends 'base.html.twig' %}

{% block title %}Détails du projet{% endblock %}

{% block body %}
    <h1>Détails du projet</h1>

    {% include 'project/_addFeatures.html.twig' %}

    {{ form_start(form) }}
    {% include 'project/_form.html.twig' with {'buttonName': 'Enregistrer les modifications'} %}
    {{ form_end(form) }}

{% endblock %}

{% block stylesheets %}
    {{ encore_entry_link_tags('app') }}
    {{ encore_entry_link_tags('projectDetails') }}
    {{ encore_entry_link_tags('easyAutocomplete') }}
{% endblock %}
{% block javascripts %}
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    {{ encore_entry_script_tags('app') }}
    {{ encore_entry_script_tags('easyautocomplete') }}
{% endblock %}
